<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="322.0" prefWidth="295.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.swdc.websdk.views.EndpointListController">
   <children>
      <HBox alignment="TOP_RIGHT" maxHeight="48.0" minHeight="36.0" prefHeight="36.0" prefWidth="200.0" spacing="8.0" VBox.vgrow="NEVER">
         <children>
            <HBox prefHeight="36.0" prefWidth="175.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button id="actions" minHeight="28.0" minWidth="28.0" mnemonicParsing="false" onAction="#onShowMenu" prefHeight="28.0" prefWidth="28.0" text="Button" />
               </children>
            </HBox>
            <Button id="remove" minHeight="28.0" minWidth="28.0" mnemonicParsing="false" onAction="#onRemove" prefHeight="28.0" prefWidth="28.0" text="Remove" />
            <Button id="add" minHeight="28.0" minWidth="28.0" mnemonicParsing="false" onAction="#onAdd" prefHeight="28.0" prefWidth="28.0" text="Add" />
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" styleClass="request-list" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
